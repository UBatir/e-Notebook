<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="editText">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:autofillHints" tools:targetApi="o"></item>
    </style>

    <style name="BottomAppBarOverlay">
        <item name="colorControlNormal">@color/black</item>
    </style>

    <style name="NumberInPasscode">
        <item name="android:layout_width">54dp</item>
        <item name="android:layout_height">54dp</item>
        <item name="android:background">@drawable/layout_selector_tran</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>