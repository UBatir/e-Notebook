<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/reportFragment2">

    <fragment
        android:id="@+id/reportFragment2"
        android:name="com.example.enotebook.screens.main.customer.ListNameFragment"
        android:label="fragment_report"
        tools:layout="@layout/fragment_list_name" >
        <action
            android:id="@+id/action_reportFragment2_to_addContactFragment"
            app:destination="@id/addContactFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_graph"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/addContactFragment"
        android:name="com.example.enotebook.screens.main.customer.addCustomer.AddCustomerFragment"
        android:label="AddContactFragment" >
        <action
            android:id="@+id/action_addContactFragment_to_reportFragment2"
            app:destination="@id/reportFragment2"
            app:launchSingleTop="true"
            app:popUpTo="@+id/nav_graph"
            app:popUpToInclusive="true"/>
    </fragment>
</navigation>